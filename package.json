{
  "name": "assemble-permalinks",
  "description": "Assemble plugin for easily creating permalinks (Assemble ^0.6.0)",
  "version": "0.6.0",
  "homepage": "https://github.com/assemble/assemble-permalinks",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)"
  ],
  "repository": "assemble/assemble-permalinks",
  "bugs": {
    "url": "https://github.com/assemble/assemble-permalinks/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.3.0",
    "isobject": "^3.0.0",
    "lazy-cache": "^2.0.2",
    "mixin-deep": "^1.2.0",
    "placeholders": "^1.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "assemble-core": "^0.31.0",
    "assemble-loader": "^1.0.5",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3"
  },
  "keywords": [
    "assemble",
    "assembleplugin",
    "blog",
    "dest",
    "destination",
    "filepath",
    "format",
    "link",
    "path",
    "permalink",
    "permalinks",
    "permanent",
    "plugin",
    "post",
    "pretty",
    "rewrite",
    "rewriting",
    "route",
    "routes",
    "routing",
    "uri",
    "url",
    "url-routing"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "assemble",
        "templates"
      ]
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
